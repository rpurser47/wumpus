{
  "version": "0.2",
  "language": "en",
  "words": [
    "wumpus",
    "confetti",
    "nextjs",
    "turbo",
    "eslint",
    "prettier",
    "typecheck",
    "cspell",
    "tsx",
    "tsc",
    "jsx",
    "jsdom",
    "stylelint",
    "browserlist",
    "devtools",
    "npmrc",
    "husky",
    "lint-staged",
    "depcheck",
    "batcave",
    "stench",
    "idx",
    "yamllint",
    "vercel",
    "ubuntu",
    "nodejs",
    "workflow",
    "workflows",
    "actionlint",
    "checkout",
    "setup",
    "ci",
    "cd"
  ],
  "ignorePaths": [
    "node_modules/**",
    "package-lock.json",
    "public/sounds/**",
    "public/images/**",
    "coverage/**",
    ".next/**",
    ".git/**"
  ],
  "ignoreRegExpList": ["\\(.*\\)", "/\\*.*\\*/", "import\\s.+\\sfrom\\s'.+'", "require\\('.+'\\)"],
  "allowCompoundWords": true,
  "minWordLength": 3,
  "patterns": [
    {
      "name": "source-files",
      "pattern": "**/*.{ts,tsx,js,jsx,json,md,yml,yaml,css,scss}"
    }
  ]
}
